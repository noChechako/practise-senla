  ) &bull;\r\n            {' '}\r\n            {genres.join(', ')}\r\n            {' '}\r\n            &bull;\r\n            {' '}\r\n            {data.runtime}\r\n            {' '}\r\n            min\r\n            {' '}\r\n          </Span>\r\n          <i>\r\n            {data.tagline}\r\n            {' '}\r\n            <br />\r\n          </i>\r\n          <Trailer onClick={() => handleClick(data.id)}>Trailer</Trailer>\r\n          <h2>Overview</h2>\r\n          <span>{data.overview}</span>\r\n          <Modal>\r\n            <Video>{trailerUrl && <YouTube videoId={trailerUrl} opts={opts} />} <Close onClick={() => handleClose()} >&#9746;</Close>  </Video>\r\n          </Modal>\r\n\r\n        </div>\r\n      </DivFilm>\r\n      <AllCasts id={data.id} />\r\n      <AllComents id={data.id} />\r\n    </>\r\n\r\n  );\r\n};\r\n/* eslint-enable @typescript-eslint/indent */\r\n\r\nexport default FilmContent;\r\n","C:\\Users\\Евгений\\films\\my-app\\src\\Components\\Reviews\\AllComents.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\Casts\\AllCasts.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\Reviews\\Coment.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\Casts\\Cast.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\SearchMenu\\Search.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\data\\getTrailer.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\SearchMenu\\SearchFilters.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\SearchMenu\\Genres.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\data\\getGenres.js",[],"C:\\Users\\Евгений\\films\\my-app\\src\\Components\\SearchMenu\\Genre.js",[],{"ruleId":"145","replacedBy":"146"},{"ruleId":"147","replacedBy":"148"},{"ruleId":"149","replacedBy":"150"},{"ruleId":"151","replacedBy":"152"},{"ruleId":"153","replacedBy":"154"},{"ruleId":"155","replacedBy":"156"},{"ruleId":"157","severity":1,"message":"158","line":147,"column":1,"nodeType":"159","messageId":"160","end